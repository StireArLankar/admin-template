{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": "..",
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"noEmit": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2018"
	},
	"exclude": ["../../build", "../client"]
}
